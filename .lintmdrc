{
  "rules":{
    "no-empty-code": 2,
    "no-empty-url": 2,
    "no-empty-list": 2,
    "space-around-alphabet": 2,
    "no-special-characters": 2,
    "no-multiple-space-blockquote": 2,
    "correct-title-trailing-punctuation": 2,
    "no-empty-blockquote": 2,
    "use-standard-ellipsis": 2,
    "space-around-number": 2,
    "no-long-code": [2, {
      "length": 100,
      "exclude": ["dot"]
    }]
  }
}